<h1>Listing eduper_score_details</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Educational performance</th>
      <th>Sd detail</th>
      <th>Sub grade teacher</th>
      <th>Weight</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @eduper_score_details.each do |eduper_score_detail| %>
      <tr>
        <td><%= eduper_score_detail.name %></td>
        <td><%= eduper_score_detail.description %></td>
        <td><%= eduper_score_detail.educational_performance %></td>
        <td><%= eduper_score_detail.sd_detail %></td>
        <td><%= eduper_score_detail.sub_grade_teacher %></td>
        <td><%= eduper_score_detail.weight %></td>
        <td><%= link_to 'Show', eduper_score_detail %></td>
        <td><%= link_to 'Edit', edit_eduper_score_detail_path(eduper_score_detail) %></td>
        <td><%= link_to 'Destroy', eduper_score_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Eduper score detail', new_eduper_score_detail_path %>
