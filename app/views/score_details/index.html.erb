<br><br>
<% if @score_details.size != 0 %>
  <div class="content-table">
    <table>
      <thead>
        <tr>
          <th>Sd detail</th>
          <th>Educational period</th>
          <th>Sub grade teacher</th>
          <th>Educational asignature</th>
          <th>Weight</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @score_details.each do |score_detail| %>
          <tr>
            <td><%= score_detail.sd_detail_id %></td>
            <td><%= score_detail.educational_period_id %></td>
            <td><%= score_detail.sub_grade_teacher_id %></td>
            <td><%= score_detail.educational_asignature_id %></td>
            <td><%= score_detail.weight %></td>
            <td><%= link_to 'Show', score_detail %></td>
            <td><%= link_to 'Edit', edit_score_detail_path(score_detail) %></td>
            <td><%= link_to 'Destroy', score_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
<% end %>

<br>

<%= link_to 'New Score detail', new_score_detail_path %>
