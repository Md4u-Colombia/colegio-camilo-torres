<h1>Listing student_trackings</h1>

<table>
  <thead>
    <tr>
      <th>Score detail</th>
      <th>Educational period</th>
      <th>Score</th>
      <th>Compliance</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_trackings.each do |student_tracking| %>
      <tr>
        <td><%= student_tracking.score_detail_id %></td>
        <td><%= student_tracking.educational_period_id %></td>
        <td><%= student_tracking.score %></td>
        <td><%= student_tracking.compliance %></td>
        <td><%= link_to 'Show', student_tracking %></td>
        <td><%= link_to 'Edit', edit_student_tracking_path(student_tracking) %></td>
        <td><%= link_to 'Destroy', student_tracking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student tracking', new_student_tracking_path %>
